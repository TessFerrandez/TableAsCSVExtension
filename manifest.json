{
    "name": "Stock table download",
    "description": "Download tables as CSV",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "action": {
        "default_title": "Download tables as CSV",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    }
}